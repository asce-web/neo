extends _docs.tpl.jade

block append vars
  - var page = ConfDocs.DOCS.find('temp.html')

block append main
  +newSection(2, ConfDocs.DOCS.find('temp.html#site-header'))
    include _includes/_temp.site-header.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#page-header'))
    include _includes/_temp.page-header.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#highlighted-content-block'))
    include _includes/_temp.highlighted-content-block.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#hero-image'))
    include _includes/_temp.hero-image.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#other-year-callout'))
    include _includes/_temp.other-year-callout.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#program-tabs'))
    include _includes/_temp.program-tabs.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#speaker'))
    include _includes/_temp.speaker.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#important-dates'))
    include _includes/_temp.important-dates.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#person'))
    include _includes/_temp.person.pug
  +newSection(2, ConfDocs.DOCS.find('temp.html#place'))
    include _includes/_temp.place.pug

block append scripts
  script(src="/scripts/o-Tabs.js")
  script.
    $('.c-Toggle__Check').change(function () {
      $(this).parents('.c-Toggle').siblings('.o-Drawer').toggleClass('o-Drawer--open')
    })
