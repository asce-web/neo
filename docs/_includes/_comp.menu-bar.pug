figure.docs-figure
  +sitetoc({
    icons: { sub: true }
  , depth: 1
  , pages: [
      new ConfPage('Home', '#0').setIcon('insert_drive_file')
    , new ConfPage('Registration', '#0').setIcon('insert_drive_file')
        .add(new ConfPage('Why Attend', '#0'))
    , new ConfPage('Program', '#0').setIcon('insert_drive_file')
        .add(new ConfPage('Short Courses', '#0'))
        .add(new ConfPage('Technical Tours', '#0'))
    , new ConfPage('Location', '#0').setIcon('insert_drive_file')
    , new ConfPage('Speakers', '#0').setIcon('insert_drive_file')
    ]
  , classes: {
      listclasses: 'o-Flex c-MenuBar'
    , itemclasses: 'o-Flex__Item c-MenuBar__Item'
    , linkclasses: 'c-MenuBar__Link h-Block'
    , sub: {
        listclasses: 'c-MenuBar__Sub'
      , itemclasses: 'c-MenuBar__Sub__Item'
      , linkclasses: 'c-MenuBar__Link c-MenuBar__Sub__Link h-Block'
      }
    }
  })
  ul.o-List.o-Flex.c-MenuBar(style="margin-bottom: 1.5rem;")
    li.o-List__Item.o-Flex__Item.c-MenuBar__Item._hover: a.c-MenuBar__Link.h-Block(href="#0")._hover menu item hover
    li.o-List__Item.o-Flex__Item.c-MenuBar__Item._hover: a.c-MenuBar__Link.h-Block(href="#0")._active menu item active
    li.o-List__Item.o-Flex__Item.c-MenuBar__Item: a.c-MenuBar__Link.h-Block menu item disabled
  pre.docs-pre.
    &lt;ul class="o-List  o-Flex  #[u c-MenuBar]"&gt;
      &lt;li class="o-List__Item  o-Flex__Item  #[u c-MenuBar__Item]"&gt;
        &lt;a class="#[u c-MenuBar__Link]  h-Block"&gt;
          &lt;span&gt;Registration&lt;/span&gt;
          &lt;i class="material-icons  #[u c-MenuBar__Icon]  -d-n-nM"&gt;expand_more&lt;/i&gt;
        &lt;/a&gt;
        &lt;ul class="o-List  #[u c-MenuBar__Sub]"&gt;
          &lt;li class="o-List__Item  #[u c-MenuBar__Sub__Item]"&gt;
            &lt;a class="#[u c-MenuBar__Sub__Link]  h-Block"&gt;
              &lt;span&gt;Why Attend&lt;/span&gt;
            &lt;/a&gt;
          &lt;/li&gt;
          ...
        &lt;/ul&gt;
      &lt;/li&gt;
      ...
    &lt;/ul&gt;
